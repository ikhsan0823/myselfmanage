{
    "version": 2,
    "builds": [
      {
        "src": "./express-server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "./socket-server/socketIO.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      
      {
        "src": "./express-server/(.*)",
        "dest": "index.js"
      },
      {
        "src": "./socket-server/(.*)",
        "dest": "socketIO.js"
      }
    ],
    "rewrites": [
      {
        "src": "./express-server/(.*)",
        "dest": "index.js",
      }
    ]
  }